{
  "name": "epro-rest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node-sass public/stylesheets/sass/ -o public/stylesheets/css/; nodemon ./bin/www --ignore 'public/*.json'",
    "seed-patients": "mongoimport --db epro --collection patients --drop --file seeds/patients.json",
    "seed-procedures": "mongoimport --db epro --collection procedures --drop --file seeds/procedures.json",
    "build-css": "node-sass public/stylesheets/sass/ -o public/stylesheets/css/"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.5.2",
    "express": "~4.14.0",
    "mongoose": "^4.7.7",
    "morgan": "~1.7.0",
    "node-sass-middleware": "0.9.8",
    "serve-favicon": "~2.3.0",
    "survey-jquery": "^0.11.0"
  }
}
