<html>
	<head>
		<% include ../partials/head %>
	</head>
	<body>
		<header>
			<% include ../partials/header %>
		</header>
		<div class="container main-wrapper">
			<h1 class="pg-title">procedures#index</h1>
			<div class="table-responsive">
				<table class="table table-hover">
					<thead>
						<th>#</th>
						<th>Name</th>
						<th>Excerpt</th>
						<th>Department</th>
					</thead>
					<tbody>					
						<% for(var i = 0; i < procedures.length; i++) { %>
							<tr>
								<td>
									<%= i+1	 %>
								</td>
								<td>
									<%= procedures[i].name+" "+procedures[i].name %>
								</td>
								<td>
									<%= procedures[i].excerpt %>
								</td>
								<td>
									<%= procedures[i].department %>
								</td>
							</tr>
						<% } %>
					</tbody>
				</table>
			</div>
		<footer>
			<% include ../partials/footer %>
		</footer>
	</body>
</html>